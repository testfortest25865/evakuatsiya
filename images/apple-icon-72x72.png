    $('body').on('submit', '#subscribe', function(e){
        e.preventDefault();
        $(".subscribe_msg").html("");
        $('#btn-subs').prop("disabled", true);
        var form = $(this);
        $.ajax({
            type: "post",
            url:  'Process/subscribe',
            data: form.serialize(),
            success : function(response){
                var data = JSON.parse(response);
                $(".subscribe_msg").html(data.msg).css("color",data.color);
                $('#btn-subs').prop("disabled", false);
            },
            error : function(err){
                alert("Sistem xətası");
                $('#btn-subs').prop("disabled", false);
            }
        });
        return false;
    });
    
    $('body').on('submit', '#contact_form', function(e){
        e.preventDefault();
        $(".form-control").removeClass("is-invalid");
        $(".invalid-feedback").addClass("d-none");
        $(".invalid-feedback").removeClass("d-block");
        $(".invalid-feedback").empty();
        $(".msg").html("");
        var form = $(this);
        $.ajax({
            type: "post",
            url:  'Process/contactfrom',
            data: form.serialize(),
            success : function(response){
                var data = JSON.parse(response);
                if(data.status=="success"){
                    $(".Input").val("");
                    $("textarea").val("");
                    // $(".msg").addClass("success-msg");
                    $(".msg").css("border-color",data.color)
                    $(".msg").html(data.msg).css("color",data.color);
                } else {
                    console.log(data);
                    data.empty.forEach(myFunction);
                    function myFunction(item, index) {
                        $("."+item).addClass("is-invalid");
                        $("."+item).siblings(".invalid-feedback").removeClass("d-none");
                        $("."+item).siblings(".invalid-feedback").addClass("d-block");
                        $("."+item).siblings(".invalid-feedback").html(data.msg).css("color",data.color);
                    }
                }
            },
            error : function(err){
                alert("Sistem xətası");
            }
        });
        return false;
    });

    
